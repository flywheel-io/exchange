{
    "name": "afq-demo",
    "label": "AFQ: Automated Fiber Quantification (DEMO)",
    "description": "AFQ was designed [by Jason D. Yeatman, et al.] to generate Tract Profiles of tissue properties for major fiber tracts in healthy and diseased brains. Online documentation can be found at: https://github.com/yeatmanlab/AFQ/wiki.",
    "maintainer": "Michael Perry <lmperry@stanford.edu>",
    "author": "Jason D. Yeatman <jyeatman@uw.edu>",
    "url": "https://github.com/yeatmanlab/afq",
    "source": "https://github.com/flywheel-apps/afq-demo",
    "license": "GPL-2.0",
    "flywheel": "0",
    "version": "0.1.0",
    "inputs": {
        "diffusion": {
            "base": "file",
            "type": {
                "enum": [
                    "nifti"
                ]
            }
        },
        "bvec": {
            "base": "file",
            "type": {
                "enum": [
                    "bvec"
                ]
            }
        },
        "bval": {
            "base": "file",
            "type": {
                "enum": [
                    "bval"
                ]
            }
        }
    },
    "config": {
        "cutoffLower": {
            "default": 10,
            "type": "integer"
        },
        "cutoffUpper": {
            "default": 90,
            "type": "integer"
        },
        "cleanFibers": {
            "default": 1,
            "type": "integer"
        },
        "cleanIter": {
            "default": 5,
            "type": "integer"
        },
        "maxDist": {
            "default": 5,
            "type": "integer"
        },
        "maxLen": {
            "default": 4,
            "type": "integer"
        },
        "numberOfNodes": {
            "default": 100,
            "type": "integer"
        },
        "clip2rois": {
            "default": 1,
            "type": "integer"
        },
        "fiberWeighting": {
            "default": 1,
            "type": "integer"
        },
        "cleanClippedFibers": {
            "default": 0,
            "type": "integer"
        },
        "savefigs": {
            "default": 0,
            "type": "integer"
        },
        "computeCSD": {
            "default": 0,
            "type": "integer"
        },
        "computenorms": {
            "default": 1,
            "type": "integer"
        },
        "normalization": {
            "type": "string",
            "default": "spm"
        },
        "imresample": {
            "default": 0,
            "type": "integer"
        },
        "track_algorithm": {
            "type": "string",
            "default": "mrdiffusion"
        },
        "track_stepSizeMm": {
            "default": 1,
            "type": "integer"
        },
        "track_faThresh": {
            "default": 0.2,
            "type": "number"
        },
        "track_minLengthThreshMm": {
            "default": 50,
            "type": "integer"
        },
        "track_maxLengthThreshMm": {
            "default": 250,
            "type": "integer"
        },
        "track_angleThresh": {
            "default": 30,
            "type": "integer"
        },
        "track_wPuncture": {
            "default": 0.2,
            "type": "number"
        },
        "track_whichAlgorithm": {
            "default": 1,
            "type": "integer"
        },
        "track_whichInterp": {
            "default": 1,
            "type": "integer"
        },
        "track_offsetJitter": {
            "default": 0,
            "type": "integer"
        },
        "track_seedVoxelOffset_1": {
            "default": 0.25,
            "type": "number"
        },
        "track_seedVoxelOffset_2": {
            "default": 0.75,
            "type": "number"
        },
        "track_faMaskThresh": {
            "default": 0.3,
            "type": "number"
        },
        "track_nfibers": {
            "default": 500000,
            "type": "integer"
        }
    },
    "custom": {
        "docker-image": "flywheel/afq-demo"
    }
}
