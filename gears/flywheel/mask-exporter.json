  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": false
    },
    "input-file": {
      "base": "file",
      "description": "NIfTI or DICOM file with ROI drawn in OHIF Viewer (Required).",
      "optional": false,
      "type": {
        "enum": [
          "nifti",
          "dicom"
        ]
      }
    }
  },
