          },
          "skull": {
            "type": "boolean"
          },
          "output_type": {
            "default": "NIFTI_GZ",
            "enum": [
              "NIFTI_PAIR",
              "NIFTI_PAIR_GZ",
              "NIFTI_GZ",
              "NIFTI"
            ],
            "type": "string"
          },
          "threshold": {
            "type": "boolean"
          },
          "robust": {
