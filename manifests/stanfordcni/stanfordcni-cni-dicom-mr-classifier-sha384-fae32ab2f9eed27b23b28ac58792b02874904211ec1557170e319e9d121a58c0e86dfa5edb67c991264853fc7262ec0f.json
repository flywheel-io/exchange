{
  "gear": {
    "name": "cni-dicom-mr-classifier",
    "label": "CNI: DICOM MR Classifier",
    "description": "Extract metadata and determine classification from GE DICOM data.",
    "maintainer": "Michael Perry <lmperry@stanford.edu>",
    "author": "Michael Perry <lmperry@stanford.edu>",
    "url": "https://cni.stanford.edu",
    "source": "https://github.com/cni/cni-dicom-mr-classifier",
    "license": "Apache-2.0",
    "flywheel": "0",
    "version": "1.0.0",
    "custom": {
      "docker-image": "stanfordcni/cni-dicom-mr-classifier:1.0.0"
    },
    "config": {
      "timezone": {
        "description": "Time Zone to which all timestamps should be localized. This will set the default time zone in the Gear and thus localize the timestamps to that time zone. Examples: 'America/Los_Angeles', 'America/New_York'. [default = 'America/Los_Angeles'].",
        "type": "string",
        "default": "America/Los_Angeles"
      },
      "sync": {
        "description": "Synchronize classification definitions with the latest known updates found at cni/cni-dicom-mr-classifier [default = true].",
        "type": "boolean",
        "default": true
      }
    },
    "inputs": {
      "dicom": {
        "base": "file",
        "type": {
          "enum": [
            "dicom"
          ]
        },
        "description": "Archive (.zip) containing DICOM files."
      },
      "classifications": {
        "base": "context"
      }
    }
  },
  "exchange": {
    "git-commit": "205af71eb2ac2e51366c5ac58d1e19979ffa1a26",
    "rootfs-hash": "sha384:fae32ab2f9eed27b23b28ac58792b02874904211ec1557170e319e9d121a58c0e86dfa5edb67c991264853fc7262ec0f",
    "rootfs-url": "https://storage.googleapis.com/flywheel-exchange/stanfordcni-cni-dicom-mr-classifier-sha384-fae32ab2f9eed27b23b28ac58792b02874904211ec1557170e319e9d121a58c0e86dfa5edb67c991264853fc7262ec0f.tgz"
  },
  "invocation-schema": {
    "$schema": "http://json-schema.org/draft-04/schema#",
    "required": [
      "config",
      "inputs"
    ],
    "type": "object",
    "properties": {
      "inputs": {
        "required": [
          "dicom",
          "classifications"
        ],
        "type": "object",
        "properties": {
          "dicom": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "dicom"
                ]
              }
            }
          },
          "classifications": {
            "required": [
              "base",
              "found",
              "value"
            ],
            "type": "object",
            "properties": {
              "found": {
                "type": "boolean"
              },
              "base": {
                "type": "string"
              },
              "value": {}
            }
          }
        }
      },
      "config": {
        "required": [
          "timezone",
          "sync"
        ],
        "type": "object",
        "properties": {
          "timezone": {
            "default": "America/Los_Angeles",
            "type": "string"
          },
          "sync": {
            "default": true,
            "type": "boolean"
          }
        }
      }
    },
    "title": "Invocation manifest for CNI: DICOM MR Classifier"
  }
}
