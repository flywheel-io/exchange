{
  "gear": {
    "name": "cni-dicom-mr-classifier",
    "label": "CNI: DICOM MR Classifier",
    "description": "Extract metadata and determine classification from GE DICOM data.",
    "maintainer": "Michael Perry <lmperry@stanford.edu>",
    "author": "Michael Perry <lmperry@stanford.edu>",
    "url": "https://cni.stanford.edu",
    "source": "https://github.com/cni/cni-dicom-mr-classifier",
    "license": "Apache-2.0",
    "flywheel": "0",
    "version": "2.1.0",
    "custom": {
      "docker-image": "stanfordcni/cni-dicom-mr-classifier:2.1.0",
      "flywheel": {
        "suite": "Stanford CNI"
      }
    },
    "config": {
      "timezone": {
        "description": "Time Zone to which all timestamps should be localized. This will set the default time zone in the Gear and thus localize the timestamps to that time zone. Examples: 'America/Los_Angeles', 'America/New_York'. [default = 'America/Los_Angeles'].",
        "type": "string",
        "default": "America/Los_Angeles"
      },
      "sync": {
        "description": "Synchronize classification definitions with the latest known updates found at cni/cni-dicom-mr-classifier [default = true].",
        "type": "boolean",
        "default": true
      }
    },
    "inputs": {
      "dicom": {
        "base": "file",
        "type": {
          "enum": [
            "dicom"
          ]
        },
        "description": "Archive (.zip) containing DICOM files."
      },
      "classifications": {
        "base": "context"
      }
    }
  },
  "exchange": {
    "git-commit": "2aaa6a7664b3dc76c68d0bcda4246332d929cfc1",
    "rootfs-hash": "sha384:56d8778276a2c004ce14440603f275267ba1092fca4fbc3f56a96dfd70833241fe162914153905c216b96953bc30ec5e",
    "rootfs-url": "https://storage.googleapis.com/flywheel-exchange/stanfordcni-cni-dicom-mr-classifier-sha384-56d8778276a2c004ce14440603f275267ba1092fca4fbc3f56a96dfd70833241fe162914153905c216b96953bc30ec5e.tgz"
  },
  "invocation-schema": {
    "title": "Invocation manifest for CNI: DICOM MR Classifier",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "config": {
        "type": "object",
        "properties": {
          "timezone": {
            "type": "string",
            "default": "America/Los_Angeles"
          },
          "sync": {
            "type": "boolean",
            "default": true
          }
        },
        "required": [
          "timezone",
          "sync"
        ]
      },
      "inputs": {
        "type": "object",
        "properties": {
          "dicom": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "dicom"
                ]
              }
            }
          },
          "classifications": {
            "type": "object",
            "properties": {
              "base": {
                "type": "string"
              },
              "found": {
                "type": "boolean"
              },
              "value": {}
            },
            "required": [
              "base",
              "found",
              "value"
            ]
          }
        },
        "required": [
          "dicom",
          "classifications"
        ]
      }
    },
    "required": [
      "config",
      "inputs"
    ]
  }
}
