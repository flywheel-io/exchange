        "base": "api-key"
      },
      "contrast": {
        "base": "file",
        "description": " json file of contrasts",
        "optional": true
      },
      "designfile": {
        "base": "file",
        "description": "designfile: list all modules to run"
      },
      "eventfile": {
        "base": "file",
        "description": "event times files ",
        "optional": true
      },
      "fmriprepdir": {
        "base": "file",
        "description": "FMRIprep output directory",
        "optional": true
      },
      "img": {
        "base": "file",
