{
  "gear": {
    "author": "Flywheel",
    "cite": "",
    "command": "poetry run python run.py",
    "config": {
      "threshold_percentile": {
        "default": 98.5,
        "description": "The percentile at which to threshold maximum values",
        "type": "number"
      },
      "invert_image": {
        "description": "Inversion of the output image.",
        "type": "boolean",
        "default": true
      },
      "verbose": {
        "description": "Whether to include verbose output from dcm2niix call. Options: true, false (default).",
        "type": "boolean",
        "default": false
      },
      "debug": {
        "default": false,
        "description": "Log debug messages",
        "type": "boolean"
      }
    },
    "custom": {
      "flywheel": {
        "suite": "Conversion"
      },
      "gear-builder": {
        "category": "converter",
        "image": "flywheel/dcm-to-mips:0.1.0"
      }
    },
    "description": "Convert DICOM file into PNG images using Maximum Intensity Projection(MIP) technique.",
    "environment": {
      "LD_LIBRARY_PATH": "/usr/lib/fsl/5.0",
      "FSLDIR": "/usr/share/fsl/5.0",
      "PATH": "/usr/lib/fsl/5.0:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
      "FSLOUTPUTTYPE": "NIFTI_GZ"
    },
    "inputs": {
      "dicom_input": {
        "base": "file",
        "description": "Main input file for the Gear. This requires a DICOM archive ('<dicom>.zip').",
        "optional": false,
        "type": {
          "enum": [
            "dicom"
          ]
        }
      }
    },
    "label": "DCM to MIPS",
    "license": "MIT",
    "maintainer": "Flywheel <support@flywheel.io>",
    "name": "dcm-to-mips",
    "source": "https://gitlab.com/flywheel-io/flywheel-apps/dcm-to-mips",
    "url": "https://gitlab.com/flywheel-io/flywheel-apps/dcm-to-mips",
    "version": "0.1.0"
  },
  "exchange": {
    "git-commit": "98f18bacf880546ab5208a9480c6eed8eeea065e",
    "rootfs-hash": "sha384:4832f0b79ce817dcf0a96d24babc2463c849110345b9b8fe794f95a137caa7dc31d4a843887f1f65043791e95b838f9f",
    "rootfs-url": "https://storage.googleapis.com/flywheel-exchange/flywheel-dcm-to-mips-sha384-4832f0b79ce817dcf0a96d24babc2463c849110345b9b8fe794f95a137caa7dc31d4a843887f1f65043791e95b838f9f.tgz"
  },
  "invocation-schema": {
    "title": "Invocation manifest for DCM to MIPS",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "config": {
        "type": "object",
        "properties": {
          "threshold_percentile": {
            "default": 98.5,
            "type": "number"
          },
          "invert_image": {
            "type": "boolean",
            "default": true
          },
          "verbose": {
            "type": "boolean",
            "default": false
          },
          "debug": {
            "default": false,
            "type": "boolean"
          }
        },
        "required": [
          "threshold_percentile",
          "invert_image",
          "verbose",
          "debug"
        ]
      },
      "inputs": {
        "type": "object",
        "properties": {
          "dicom_input": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "dicom"
                ]
              }
            }
          }
        },
        "required": [
          "dicom_input"
        ]
      }
    },
    "required": [
      "config",
      "inputs"
    ]
  }
}
